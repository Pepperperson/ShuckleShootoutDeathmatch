ACTOR ShuckleNoPuff
{
	States
	{
		Spawn:
			TNT1 A 0
			Stop
	}
}

ACTOR ShuckleWeaponBase : DoomWeapon
{
	Weapon.AmmoSpRefill 10
	Weapon.AmmoDmRefill 1
	Weapon.AmmoDmScale 1
	Inventory.RespawnTics 105
	+NOAUTOAIM
	States
	{
		Ready:
			"----" A 1 A_WeaponReady
			Loop
		Deselect:
			"----" A 0 A_ZoomFactor(1)
			"----" A 0 A_TakeInventory("ZoomVar",1)
			"----" A 0 A_Lower
			"----" A 0 A_Lower
			"----" A 1 A_Lower
			Loop
		Select:
			"----" A 0 A_ZoomFactor(1)
			"----" A 0 A_TakeInventory("ZoomVar",1)
			"----" A 0 A_Raise
			"----" A 0 A_Raise
			"----" A 1 A_Raise
			Loop
		Fire:
			"----" A 10 A_FireBullets(0,0,1,1)
			"----" A 0 A_Refire
			Goto Ready
		AltFire:
			"----" A 0 A_JumpIfInventory("ZoomVar",1,"AltFireScope")
			"----" A 0 A_GiveInventory("ZoomVar",1)
			"----" A 10 A_ZoomFactor(3)
			Goto Ready
		AltFireScope:
			"----" A 0 A_TakeInventory("ZoomVar",1)
			"----" A 10 A_ZoomFactor(1)
			Goto Ready
	}
}

ACTOR MudBomb : ShuckleWeaponBase 15001
{
	//$Not Angled
	//$Category "Shuckle Shootout Weapons"
	//$Title "Mud Bomb(STARTER, DO NOT PLACE)"
	Weapon.SelectionOrder 1900
	Weapon.AmmoUse 1
	Weapon.AmmoGive 20
	Weapon.AmmoDmRefill 10
	Weapon.AmmoType "PP"
	Obituary "%o got Mud Bombed."
	+WEAPON.WIMPY_WEAPON
	Inventory.Pickupmessage "PP restored for Mud Bomb!"
	Tag "Mud Bomb"
	Scale 0.4
	States
	{
	Ready:
		TNT1 A 1 A_WeaponReady
		Loop
	Deselect:
		TNT1 A 0 A_Lower
		TNT1 A 0 A_Lower
		TNT1 A 1 A_Lower
		Loop
	Select:
		TNT1 A 0 A_Raise
		TNT1 A 0 A_Raise
		TNT1 A 1 A_Raise
		Loop
	Fire:
		MUDB A 0 A_PlaySound("weapons/mudbomb", CHAN_WEAPON)
		MUDB A 2 A_FireCustomMissile("MudBombProjectile",0,1)
		MUDB BCD 2
		TNT1 A 8
		TNT1 A 5 A_ReFire
		Goto Ready
	Spawn:
		AMPP A -1
		Stop
	}
}
ACTOR OranBerry : ShuckleWeaponBase 15002
{
	//$Not Angled
	//$Category "Shuckle Shootout Weapons"
	//$Title "Oran Berry (STARTER, DO NOT PLACE)"
	Weapon.SelectionOrder 2000
	Weapon.AmmoUse 1
	Weapon.AmmoGive 1
	Weapon.AmmoDmRefill 1
	Weapon.AmmoType "Oran"
	Obituary "%o randomly dropped dead."
	Inventory.Pickupmessage "Got an Oran Berry!"
	Tag "Oran Berry"
	Scale 0.4
	States
	{
	Ready:
		ORAN A 1 A_WeaponReady
		Loop
	Deselect:
		ORAN A 0 A_Lower
		ORAN A 0 A_Lower
		ORAN A 1 A_Lower
		Loop
	Select:
		ORAN A 0 A_Raise
		ORAN A 0 A_Raise
		ORAN A 1 A_Raise
		Loop
	Fire:
		ORAN A 0 A_JumpIfInventory("Oran",0,"Ready")
		ORAN A 0 A_TakeInventory("Oran",1)
		ORAN A 4 A_PlaySound("healstart",CHAN_WEAPON)
		ORAN BCDD 4
		ORAN F 4 A_PlaySound("heal",CHAN_WEAPON)
		ORAN F 0 A_GiveInventory("HealthCandy")
		ORAN GH 4
		Goto Ready
	Spawn:
		PKOB A -1
		Stop
	}
}
ACTOR ShuckleFist : Weapon 15003
{
	//$Not Angled
	//$Category "Shuckle Shootout Weapons"
	//$Title "Fists (STARTER, DO NOT PLACE)"
	Weapon.SelectionOrder 3700
	Weapon.Kickback 100
	Obituary "%o was humiliated!!"
	Tag "Fists"
	+WEAPON.WIMPY_WEAPON
	+WEAPON.MELEEWEAPON
	States
	{
	Ready:
		SFST A 1 A_WeaponReady
		Loop
	Deselect:
		SFST A 0 A_Lower
		SFST A 0 A_Lower
		SFST A 1 A_Lower
		Loop
	Select:
		SFST A 0 A_Raise
		SFST A 0 A_Raise
		SFST A 1 A_Raise
		Loop
	Fire:
		SFST B 4 A_CustomPunch(15,True,0,None,64,0)
		SFST C 4
		SFST E 0 A_ReFire("FireRepeat")
		Goto Ready
	FireRepeat:
		SFST D 4 A_CustomPunch(15,True,0,None,64,0)
		SFST E 4
		SFST E 0 A_ReFire
		Goto Ready
	AltFire:
		SFST F 3
		SFST G 0 A_FireBullets(0,0,1,1,"ShuckleNoPuff",FBF_NORANDOM)
		SFST G 24 A_PlaySound("middle5",CHAN_WEAPON)
		SFST F 3
		Goto Ready
	}
}
