actor Oran : Ammo 16000
{
	//$Not Angled
	//$Category "Shuckle Shootout Ammo"
	//$Title "Oran Berry"
	Inventory.PickupMessage "Got an Oran Berry!"
	Inventory.Amount 1
	Inventory.MaxAmount 10
	Inventory.Icon "CLIPA0"
	Ammo.BackpackAmount 1
	Ammo.BackpackMaxAmount 1
	Scale 0.4
	states
	{
	Spawn:
		PKOB A -1
		stop
	}
}
