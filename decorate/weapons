ACTOR ShuckleShotgun : ShuckleWeaponBase 15005
{
	//$Not Angled
	//$Category "Shuckle Shootout Weapons"
	//$Title "Shotgun"
	Scale 0.4
	Weapon.SelectionOrder 1300
	Weapon.AmmoUse 1
	Weapon.AmmoGive 10
	Weapon.AmmoType "ShuckleShells"
	Weapon.AmmoDmRefill 5
	Inventory.PickupMessage "Got the Shuckle Shotgun!"
	Obituary "%o discovered what it's like to have 87 bullets up their asshole."
	Tag "Shuckle Shotgun"
	States
	{
	Ready:
		SHST A 1 A_WeaponReady
		Loop
	Deselect:
		SHST A 0 A_Lower
		SHST A 0 A_Lower
		SHST A 1 A_Lower
		Loop
	Select:
		SHST A 0 A_Raise
		SHST A 0 A_Raise
		SHST A 1 A_Raise
		Loop
	Fire:
		SHST B 0 A_PlaySound("weapons/shuckleshotgun",CHAN_WEAPON)
		SHST B 3 A_FireBullets(4,4,6,8,BulletPuff,FBF_NORANDOM|FBF_USEAMMO)
		SHST C 6
		SHST A 3 A_ReFire("Fire2")
		SHST DEF 3 A_ReFire("Fire2")
		SHST G 0 A_PlaySound("weapons/shuckleshotgunreload",CHAN_WEAPON)
		SHST GH 3 A_ReFire("Fire2")
		SHST I 3 A_ReFire
		Goto Ready
	Fire2:
		SHST B 0 A_PlaySound("weapons/shuckleshotgun",CHAN_WEAPON)
		SHST B 3 A_FireBullets(4,4,6,8,BulletPuff,FBF_NORANDOM|FBF_USEAMMO)
		SHST C 6
		SHST A 3
		SHST DEF 3
		SHST G 3 A_PlaySound("weapons/shuckleshotgunreload",CHAN_WEAPON)
		SHST H 3
		SHST I 3 A_ReFire
		Goto Ready
	Spawn:
		PKSG A -1
		Stop
	}
}
